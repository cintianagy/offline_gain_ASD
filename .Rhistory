study1_SL_VP_long <- pivot_longer(
data = study1_SL_VP,
cols = learning_type,
names_to = c("Learning_type"),
values_to = "Learning_score")
study1_SL_VP_long$Learning_type[study1_SL_VP_long$Learning_type == "SL_score"] <- 1
study1_SL_VP_long$Learning_type[study1_SL_VP_long$Learning_type == "VP_score"] <- 2
# merge datasets
study2_SL_VP <- merge(study2_SL_long, study2_VP_long, by = c("Group", "Subject", "Learning_phase"))
learning_type <- c("SL_score", "VP_score")
study2_SL_VP_long <- pivot_longer(
data = study2_SL_VP,
cols = learning_type,
names_to = c("Learning_type"),
values_to = "Learning_score")
study2_SL_VP_long$Learning_type[study2_SL_VP_long$Learning_type == "SL_score"] <- 1
study2_SL_VP_long$Learning_type[study2_SL_VP_long$Learning_type == "VP_score"] <- 2
ggplot(study1_SL_VP_long, aes(x = Learning_type, y = Learning_score, fill = factor(Group, levels = c("lower AQ", "higher AQ"))), show.legend = FALSE) +
geom_flat_violin(aes(fill = factor(Group, levels = c("lower AQ", "higher AQ"))), position = position_nudge(x = .1, y = 0), adjust = 1.5, trim = FALSE, alpha = .5, colour = NA, show.legend = TRUE) +
geom_point(aes(x = as.numeric(Learning_type) - .15, colour = factor(Group, levels = c("lower AQ", "higher AQ"))), position = position_jitter(width = .05), size = 1, shape = 20, show.legend = FALSE) +
geom_boxplot(aes(x = Learning_type, y = Learning_score, fill = factor(Group, levels = c("lower AQ", "higher AQ"))),outlier.shape = NA, alpha = .5, width = .1, colour = "black", show.legend = FALSE) +
scale_colour_brewer(palette = "Dark2") +
scale_fill_brewer(palette = "Dark2") +
xlab("Learning type") +
ylab("Mean offline change score (ms)") +
scale_x_discrete(labels = c("Statistical learning", "Visuomotor performance")) +
theme_classic() +
labs(fill = "Group")
